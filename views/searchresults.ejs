<div class="row">
    <div class="col-md-12">
      <div class="panel panel-default">
        <div class="panel-heading clearfix">
          <h3 class="panel-title">Found <%= results.total %> results:</h3>
        </div>
		<% if(results && results.total) { %>
			<br />
			<table style='text-align:center;' class="table table-hover">
				<thead>
		        <tr>
              <th>Pic</th>
		          <th>Model Number</th>
		          <th>Manufacturer</th>
		          <th>Current Price</th>
		        </tr>
      	</thead>
      	<tbody>
					<% for(var i = 0; i < results.total; i++) { %>
						<% var result = results.products[i]; %>

						<tr <tr class='clickable-row' onclick="document.location = '/model?modelNumber=<%=result.modelNumber%>';">
              <td><img src='<%= result.largeImage %>'><br><strong><%= result.modelNumber %></strong></td>
							<td><%= result.name %></td>
							<td><%= result.manufacturer %><br><%= result.upc %></td>
							<td><%= result.salePrice %></td>
						</tr>

					<% } %>
				</tbody>
		    </table>


		<% } else { %>
			<br />
			No results returned!
		<% } %>
	  </div>
	</div>
  </div>
